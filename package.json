{
  "name": "v4",
  "description": "Personal Website V4",
  "version": "1.0.0",
  "author": "Brittany Chiang <brittany.chiang@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/bchiang7/v4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "start": "gatsby develop",
    "precommit": "pretty-quick --staged && lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "rules": {
      "header-max-length": [
        2,
        "always",
        72
      ],
      "subject-case": [
        2,
        "always",
        "sentence-case"
      ]
    }
  },
  "dependencies": {
    "animejs": "^2.2.0",
    "babel-plugin-styled-components": "^1.9.0",
    "gatsby": "^2.0.55",
    "gatsby-image": "^2.0.20",
    "gatsby-plugin-google-analytics": "^2.0.7",
    "gatsby-plugin-manifest": "^2.0.10",
    "gatsby-plugin-netlify": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.17",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-robots-txt": "^1.3.0",
    "gatsby-plugin-sharp": "^2.0.13",
    "gatsby-plugin-sitemap": "^2.0.2",
    "gatsby-plugin-styled-components": "^3.0.3",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.0.0",
    "gatsby-source-filesystem": "^2.0.9",
    "gatsby-transformer-remark": "^2.1.12",
    "gatsby-transformer-sharp": "^2.1.8",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-anchor-link-smooth-scroll": "^1.0.11",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-transition-group": "^2.5.0",
    "scrollreveal": "^4.0.5",
    "styled-components": "^4.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@upstatement/eslint-config": "0.3.0",
    "@upstatement/prettier-config": "^0.3.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0"
  }
}
